<%# 页面切换 %>
<% if (page.total > 1){ %>
    <section class="pagination">
        <% if (page.prev) { %>
            <a class="extend prev" rel="prev" name="page" href="<%- url_for(page.prev_link) %>">
                <i class="fas fa-angle-left"></i>
            </a>
        <% }else { %>
            <span class="extend prev disable">
                <i class="fas fa-angle-left"></i>
            </span>
        <% } %>
        <div class="page-number center"><%- page.current %> / <%- page.total %></div>
        <% if (page.next) { %>
            <a class="extend next" rel="next" href="<%- url_for(page.next_link) %>">
                <i class="fas fa-angle-right"></i>next
            </a>
        <% } else { %>
            <span class="extend next disable">
                <i class="fas fa-angle-right"></i>
            </span>
        <% } %>
    </section>
<% } %>
